                                test="count(gmd:hierarchyLevel) = 1">
               <xsl:attribute name="location">
                  <xsl:apply-templates select="." mode="schematron-select-full-path"/>
               </xsl:attribute>
               <svrl:text>Resource type is mandatory. One shall be provided.</svrl:text>
